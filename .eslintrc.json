{
    "extends": "eslint:recommended",
    "plugins": ["vue", "import"],
    "parser": "babel-eslint",
    "rules": {
        /* your overrides */
        "semi": ["warn", "never"],
        "indent": ["error", 4],
        "camelcase": ["warn", {"properties": "always"}],
        "no-shadow": ["error", { "builtinGlobals": true, "hoist": "functions", "allow": [] }]
    },
    "globals":{
        "window": true,
        "console": true,
        "document": true
    }
}
